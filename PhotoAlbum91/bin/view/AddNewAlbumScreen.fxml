            </Text>
            <TextField fx:id="albumname" prefHeight="33.0" prefWidth="161.0" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="addBtn" layoutX="63.0" layoutY="165.0" mnemonicParsing="false" onAction="#add" prefHeight="24.0" prefWidth="86.0" text="Add" />
